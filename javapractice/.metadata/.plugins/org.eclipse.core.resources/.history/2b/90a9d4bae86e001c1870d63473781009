package organization;

public class Employee1 extends CEO {
	private int eid;
	private String elogin;
	private String ePassword;
	
	public Employee1( int eid, String elogin,String ePassword) {
		super();
		this.eid=eid;
		 this.elogin=elogin;
		this.ePassword=ePassword;
		System.out.println("inside Employee class ");
		System.out.println("Employee details " +"ID"+eid+"login"+elogin+"password"+ePassword);
	}
	

	public static void main(String[] args) {
		Employee1 e = new Employee1 (101,"Sumit","123");

	}

}
